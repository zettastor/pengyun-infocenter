<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="py.infocenter.store.TaskRequestInfoDb" table="task_Request_info">
    <id column="taskId" name="taskId" type="long"/>
    <property column="taskType" name="taskType" type="string"/>
    <property column="taskRequest" name="taskRequest" type="java.sql.Blob"/>
    <property column="taskCreateTime" name="taskCreateTime" type="long"/>
  </class>
</hibernate-mapping>

