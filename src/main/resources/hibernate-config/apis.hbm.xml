<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="py.icshare.authorization.ApiToAuthorize" table="apis">
    <id column="name" name="apiName" type="string"/>
    <property column="category" name="category" type="string"/>
    <property column="chineseText" name="chineseText" type="string"/>
    <property column="englishText" name="englishText" type="string"/>
  </class>
</hibernate-mapping>