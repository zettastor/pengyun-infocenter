<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="py.icshare.InstanceMaintenanceInformation" table="maintenances">
    <id column="instance_id" name="instanceId"/>
    <property column="start_time" name="startTime" type="long"/>
    <property column="end_time" name="endTime" type="long"/>
    <property column="duration" name="duration" type="long"/>
    <property column="ip" name="ip" type="string"/>
  </class>
</hibernate-mapping>