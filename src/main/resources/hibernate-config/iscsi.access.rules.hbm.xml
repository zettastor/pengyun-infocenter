<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="py.icshare.iscsiaccessrule.IscsiAccessRuleInformation" table="iscsi_access_rules">
    <id column="rule_id" name="ruleId"/>
    <property column="rule_notes" name="ruleNotes" type="string"/>
    <property column="initiatorname" name="initiatorName" type="string"/>
    <property column="username" name="user" type="string"/>
    <property column="passed" name="passed" type="string"/>
    <property column="out_user" name="outUser" type="string"/>
    <property column="out_passed" name="outPassed" type="string"/>
    <property column="permission" name="permission" type="integer"/>
    <property column="status" name="status" type="string"/>
  </class>
</hibernate-mapping>