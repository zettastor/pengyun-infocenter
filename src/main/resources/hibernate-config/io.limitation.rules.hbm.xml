<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="py.io.qos.IoLimitationInformation" table="iolimitation_rules">
    <id column="rule_id" name="ruleId"/>
    <property column="ioLimitation_name" name="ioLimitationName" type="java.lang.String"/>
    <property column="limit_type" name="limitType" type="java.lang.String"/>
    <property column="io_limitation_entry" name="ioLimitationEntriesJson" type="java.sql.Blob"/>
    <property column="status" name="status" type="java.lang.String"/>
  </class>
</hibernate-mapping>