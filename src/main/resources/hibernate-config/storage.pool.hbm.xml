<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="py.informationcenter.StoragePoolInformationDb" table="storagepool_relationship">
    <id column="storagePool_id" name="poolId" type="java.lang.Long"/>
    <property column="domain_id" name="domainId" type="java.lang.Long"/>
    <property column="domain_name" name="domainName" type="java.lang.String"/>
    <property column="storagePool_name" name="poolName" type="java.lang.String"/>
    <property column="storagePool_strategy" name="poolStrategy" type="java.lang.String"/>
    <property column="storagePool_description" name="poolDescription" type="java.lang.String"/>
    <property column="archives_info" name="archivesInDatanode" type="java.sql.Blob"/>
    <property column="volumeIds_info" name="volumeIds" type="java.sql.Blob"/>
    <property column="status" name="status" type="java.lang.String"/>
    <property column="lastUpdateTime" name="lastUpdateTime" type="java.lang.Long"/>
    <property column="migration_speed_id" name="migrationRuleId" type="java.lang.Long"/>
    <property column="storage_pool_level" name="storagePoolLevel" type="java.lang.String"/>
  </class>
</hibernate-mapping>
