<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="py.icshare.VolumeDeleteDelayInformation" table="volume_delete_delay">
    <id column="volume_id" name="volumeId"/>
    <property column="time_for_delay" name="timeForDelay" type="long"/>
    <property column="stop_delay" name="stopDelay" type="java.lang.Boolean"/>
  </class>
</hibernate-mapping>