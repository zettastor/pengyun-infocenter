<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="py.icshare.authorization.PyResource" table="resources">
    <id column="resource_id" name="resourceId"/>
    <property column="resource_name" name="resourceName" type="string"/>
    <property column="resource_type" name="resourceType" type="string"/>
  </class>
</hibernate-mapping>