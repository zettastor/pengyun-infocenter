<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="py.icshare.qos.MigrationRuleRelationshipInformation" table="migration_relationship">
    <id column="relationship_id" name="relationshipId"/>
    <property column="storage_pool_id" name="storagePoolId" type="long"/>
    <property column="rule_id" name="ruleId" type="long"/>
    <property column="status" name="status" type="string"/>
  </class>
</hibernate-mapping>
